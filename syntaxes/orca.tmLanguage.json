{
    "scopeName": "source.orcaOut",
    "name": "ORCA-output",
    "patterns": [
      {
        "name": "constant.numeric.orca",
        "match": "\\b\\d+\\.?\\d*\\b"
      },
      {
        "name":"markup.heading.1.orca",
        "match":" +\\*{10,}\\s*\\r?\\n"
      },
      {
        "name":"markup.quote.orca",
        "match": "\\|\\s{1,5}\\d+\\> [^\\r?\\n]+"
      }
    ]
  }
  