{
  "name": "orcatoc",
  "displayName": "orca_toc",
  "description": "Provides an outline for ORCA files in the VS Code sidebar.",
  "version": "0.2.3",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.showOrcaOutline",
    "onLanguage:orcaOut"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
          "command": "extension.showOrcaOutline",
          "title": "Show ORCA Outline"
      },
      {
        "command": "extension.showOrcaOutlineExternal",
        "title": "Show ORCA Outline External"
      }
    ],
    "views": {
        "explorer": [
            {
                "id": "orcaFileOutline",
                "name": "ORCA FILE OUTLINE",
                "when": "resourceExtname =~ /.out$/"
            }
        ]
    },
    "languages": [{
      "id": "orcaOut",
      "extensions": [".out"],
      "aliases": ["ORCA-output", "orca-output"],
      "configuration": "./language-configuration.json"
  }]
  },

  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  },
  "repository": {
   "type": "git",
    "url": "https://github.com/liqunkang/orca_TOC.git" 
  },
  "license": "GPL-3.0",
  "publisher": "liqunKang",
  "icon": "images/orca_TOC_new.png"
}
